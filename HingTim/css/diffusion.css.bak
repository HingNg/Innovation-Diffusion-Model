circle{
	stroke:#777;
}
.node{
	cursor: pointer;;
}
/*status 0*/
//circle .susceptible{
	circle.aware{
	fill:#008000;
}
/*status 1*/
//circle .mention{
	circle.interest{
	fill:#0000FF;
}
/*status 2*/
circle .triel{
	fill:#FF0000;
}
circle.subunit-label {
  fill: #777;
  fill-opacity: .5;
  font-size: 20px;
  font-weight: 300;
  text-anchor: middle;
}
/*status 4*/
circle .infected{
	fill:#FF0000;
}
.new{
	stroke:#BCEE68;
	fill:#00E5EE;
}

.axis path,
.axis line {
fill: none;
stroke: black;
stroke-width:3;
}
.axis text {
font-family: sans-serif;
font-size: 11px;
}